{
    format      binary; // can also be in ASCII but takes more space
    class       volVectorField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      U; // Velocity in metres per second
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

