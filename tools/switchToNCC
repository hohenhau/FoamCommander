#/bin/bash

# Remove a specific keyword in a dictionary
# foamDictionary 10/U -entry "boundaryField/rotatingPropeller/value" -remove

# Set the boundary type to "fixedValue"
foamDictionary 10/U -entry "boundaryField/rotatingPropeller/type" -set "fixedValue"

# Set the boundary value to "uniform ( 0 0 0 )"
foamDictionary 10/U -entry "boundaryField/rotatingPropeller/vaue" -set "uniform ( 0 0 0 )"

